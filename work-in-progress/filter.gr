import Vec
import Bool

filter : forall n : Nat, k : Nat
       . (Int -> Bool) |(n+k)| -> Vec (n + k) Int -> Vec n Int
filter |predicate| vec =
    case vec of
      Nil -> Nil;
      (Cons x xs) ->
        (case predicate x of
          True -> Cons x (filter |predicate| xs);
          False -> filter |predicate| xs)
